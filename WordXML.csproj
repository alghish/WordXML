<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <WarningLevel>0</WarningLevel>
    <PackageId>WordXML.Utilities.Engine</PackageId>
    <Version>1.0.0</Version>
    <Authors>Mohamad Alghosh</Authors>
    <Company>Alghosh</Company>
    <Description>Library for processing word header , footer, content controller</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>Word, Bookmark, Header, Footer</PackageTags>
    <RepositoryUrl></RepositoryUrl>
